
@{
    ViewBag.Title = "Index";
}
<h3>Dashboard</h3><hr />
    <div class="row">
        <div class="col-xl-6 col-12">
            <div class="box text-center">
                <div class="box-body">
                    <p class="box-title font-size-18 mb-0"><i class="fa fa-user-graduate"></i> Student Enrolled</p>
                    <h2><span class="count">@ViewBag.std</span></h2>
                    <p>
                        <i class="mdi mdi-chevron-up"></i>
                        <span class="text-black-50">Count</span>
                    </p>
                </div>
               
            </div>
        </div>	
        <div class="col-xl-6 col-12">
            <div class="box text-center">
                <div class="box-body">
                    <p class="box-title font-size-18 mb-0"><i class="fa fa-user-edit"></i> Total Teachers</p>
                    <h2><span class="count">@ViewBag.teacher</span></h2>
                    <p>
                        <i class="mdi mdi-chevron-up"></i>
                        <span class="text-black-50">Count</span>
                    </p>
                </div>

            </div>
        </div>
        
    </div>
<hr />
<div class="box">
    <div class="box-title">Test Results</div><hr />
@Html.Action("TestResult", "Chart")
</div>

<hr />
<div class="box">
    <div class="box-title">Exam Results</div><hr />
@Html.Action("ExamResult", "Chart")
</div>

<script src="~/Scripts/jquery-3.4.1.min.js"></script>
<script src="~/Scripts/jquery.counterup.min.js"></script>
<script src="~/Scripts/jquery.waypoints.min.js"></script>
<script>
    $('.count').counterUp({
        delay:2,
        time:200
    })
</script>
